<style is:global>
	
	:root{
		--white-primary:#ffe4ea;
		--white-secondary:#e6d2d6;
		--pink-primary:#f1406a;
		--pink-secondary:#ee90a6;
		--border-color-soft:#4e4347;
		--border-color-dominant:#322c2f;
		--gray-primary:#9a7a81;
		--black-primary:#251F21;
		--black-dominant:#1a1516;
		}
	*{
		font-family: 'GitLabSans';
		-webkit-font-smoothing: antialiased;
		padding: 0;
		margin: 0;
	}
	@font-face {
  		font-family: 'GitLabSans';
  		src: url('/fonts/GitLabSans.woff2') format('woff2');
	}
	@font-face {
  		font-family: 'GitLabMono';
  		src: url('/fonts/GitLabMono.woff2') format('woff2');
	}
	a{
		text-decoration: none;
	}
	.technology-stack{
		display:grid;
		grid-template-columns:repeat(auto-fill,minmax(70px,1fr));		
		gap:4px;
	}
	.technology-icon-outline{
		fill:var(--gray-primary);
	}
	.technology-icon{
		stroke:var(--gray-primary);
	}
	.technology-icon:hover{
		transform:translateY(-2px);
		stroke:var(--pink-secondary)
	}
	.technology-icon-outline:hover{
		transform:translateY(-2px);
		fill:var(--pink-secondary)
	}

	.technology-icon, .technology-icon-outline{
		stroke-width:1px;
		width:63px;
		height: 63px;
		transition:.15s ease;
	}
	
	.contact-info{
		margin-top:24px;
		display:flex;
		align-items:center;
		gap:8px
	}

	.aditional-info-dot{
		fill:var(--gray-primary);
		height:8px;
		width:8px;
	}
	.aditional-info-icons{
		stroke-width:1.8px;
		width:17px;
		height:17px;
		stroke:var(--gray-primary)
	}
	.hero-icons{
		stroke:var(--white-secondary)
	}
	
	.section-experience{
		display: flex;
		flex-direction: column;
		gap:16px;
		position:relative;
	}
	.hero-upper-info{
		display:flex;
		gap:12px;
		margin-bottom:24px;}
	.cards{
		display:grid;
		grid-template-columns:repeat(auto-fill,minmax(180px,1fr));
		gap:8px;
	}
	.experience-card-thread{
        height: 100%;
        width:.3px;
        position: absolute;
        background-color: var(--border-color-soft);
        z-index: -1;
        left:23px;
        translate: -10px 0;
    }
	.text-smooth{
		-webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
	}
	.sections{
		display: flex;
		flex-direction: column;
	}
	.hero-about-me{
		display: flex;
		margin-bottom:32px;
		flex-direction: column;
		align-items: start;
	}
	.hero-description-mark{
		background-color: transparent;
		color:var(--pink-primary);
		font-weight:700;
	}
	.hero-presentation{
		display: flex;
		flex-direction: column;
		max-width: 700px;
	}
	.hero-subtitle{
		color:#ffe4ea;
		text-align: start;
		font-weight: 400;
		letter-spacing: -.1px;
		background:linear-gradient(45deg,#ffe4ea,#ff7491 );
		-webkit-background-clip: text;
  		-webkit-text-fill-color: transparent;
		font-size: 18px;
		font-family: 'Inter Tight';
	}
	.hero-description{
		letter-spacing: .3px;
		line-height:24px;
		color:var(--gray-primary);
	}
	.section-title-wrapper{
		display:flex;
		align-items:baseline;
		margin-bottom: 16px;
		gap:6px;
	}
	.section-title-icon{
		height:22px;
		width:22px;
		stroke-width:1.5px;
		stroke:var(--pink-secondary);
	}
	.section-title{
		font-weight: 300;
		
		font-family: 'Oswald';
		display: inline-block;
		color:#ffe4ea;
		font-size: 24px;
		text-align: start;
		background:linear-gradient(45deg,#ffe4ea,#ff7491 );
		-webkit-background-clip: text;
  		-webkit-text-fill-color: transparent;
	}
	.hero-title{
		line-height: 78px;
		font-family:'Inter';
		color:#ffe4ea;
		font-size: clamp(2.5rem,3.952vw,5.0rem);
		font-weight: 600;
		letter-spacing: -.08em;
		-webkit-font-smoothing: antialiased;
		-moz-osx-font-smoothing: grayscale;
		text-align: start;
		background:linear-gradient(45deg,#ebc9d0,#ee859b  );
		-webkit-background-clip: text;
  		-webkit-text-fill-color: transparent;
	}
	.bg-lines{
		  position:fixed;
		  left:0;
		  width:100%;
		  height: 100vh;
		  z-index: -1;
		  top:0;
        background-image: radial-gradient(#322c2e 1px, transparent 0);
        background-size: 60px 60px;
         background-position: -19px -19px;
	}
	.bg-lines-wrapper{
		position:relative;
		border-radius: 500px;
		overflow: hidden;
	}
	@media all and (max-width:640px){
		.experience-card-thread{
			left:23px;
		}
		.hero-title{
			line-height:36px;
			margin-bottom:16px;
		}
		/* .hero-about-me{
			text-align:center;
		} */
		.aditional-info-dot{
			display:none;
		}
		.contact-info .button{
			justify-content:center;
		}
		.hero-upper-info{
			margin-bottom:16px;
			overflow-x:scroll;
		}
		.hero-upper-info small{
			font-size:11px;
			white-space:nowrap;

		}
		.contact-info{
			gap:8px;
			width:100%;
			align-items:stretch;
			flex-direction:column;
		}
		.main{
			overflow:hidden;
			padding:32px 16px 0 16px !important
		}
	}
	.main{
		flex-direction: column;
		display: flex;
		align-items: center;
		padding:72px  0 0 0;
		justify-content: center;
		position: relative;
		height: 100%;
		width: 100%;
		background:radial-gradient( #251217,#000000);
		z-index: 4;
		opacity: 1;
	}
	.svg-grainy{
		top:0;
		left: 0;
		z-index: -1;
		position: absolute;
		filter:contrast(125%) brightness(110%); 
		height:100%;
		opacity:10%;
		width:100%;
	}

	h1{
        /* font-size:63.96px; */
        font-size: clamp(2.30rem,4.2vw,4.06rem);
    }
    h2{
        font-size: 45.23px;
    }
    h3{
        font-size: 1.999rem;
    }
    h4{
        font-size: 1.414rem;
    }
    small{
        font-size:14px;
    }
    p{
        font-size:16px;
    }

</style>
